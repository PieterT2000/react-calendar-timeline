    "react-router-dom": "^6.16.0",
    "react-testing-library": "^6.0.3",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
